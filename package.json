{
  "name": "graphql-cli-binding",
  "version": "0.2.0",
  "description": "Generates a static binding file for any GraphQL schema and outputs a single code file",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -d",
    "pretest": "npm run build",
    "lint": "tslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supergraphql/graphql-cli-binding.git"
  },
  "keywords": [
    "graphql-cli"
  ],
  "author": "Kim Brandwijk <kim.brandwijk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supergraphql/graphql-cli-binding/issues"
  },
  "homepage": "https://github.com/supergraphql/graphql-cli-binding#readme",
  "dependencies": {
    "chalk": "^2.3.0",
    "graphql-static-binding": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "8.5.2",
    "conventional-changelog-eslint": "^0.3.0",
    "sr-commit-analyzer": "^2.0.3",
    "sr-release-notes-generator": "^1.1.3",
    "tslint": "5.8.0",
    "typescript": "2.6.2"
  },
  "release": {
    "analyzeCommits": {
      "path": "sr-commit-analyzer",
      "preset": "eslint"
    },
   "generateNotes": {
      "path": "sr-release-notes-generator",
      "preset": "eslint"
    }
  }
}
